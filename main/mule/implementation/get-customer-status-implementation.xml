http://www.mulesoft.org/schema/mule/os http://www.mulesoft.org/schema/mule/os/current/mule-os.xsd">
<sub-flow name="get-customers-status-implementation-sub-flow" doc:id="5648c84e-39ce-4eaf-94e2-28f0cc691c8f" >
<logger level="INFO" doc:name="Start" doc:id="939dee79-4517-4e87-95a1-f6ed1e05ff29" message='Start FLOW #[flow.name] "Calling SFDC Query" #[now()]'/>
<salesforce:query doc:name="Get Customer Status" doc:id="99850ce7-1cd0-4546-8f6b-86c975fbc829" config-ref="Salesforce_Config">
<salesforce:salesforce-query ><![CDATA[select customerId__c from CustomerData__c where lastModifiedDate__c > ':lastModifiedDate']]></salesforce:salesforce-query>
<salesforce:parameters ><![CDATA[#[output application/java
---
{
"lastModifiedDate" : attributes.queryParams.lastModifiedDateTime
}]]]></salesforce:parameters>
</salesforce:query>
<ee:transform doc:name="Response Payload" doc:id="fd88a4ef-cc85-4ffa-bdb8-606ed76bdc8e" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd" >
<ee:message >
<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
payload map ((item, index) ->
{
	"customerId__c": item.customerId__c
}
)]]></ee:set-payload>
</ee:message>
</ee:transform>
		<logger level="INFO" doc:name="End" doc:id="4016e139-ca2d-46b4-98c6-8cba705ce65a" message='END  "SFDC Query Calling" -- #[now()]' />
</sub-flow>
</mule>